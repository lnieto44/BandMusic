<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/nestedScrollView"
    android:background="@drawable/fondo"
    android:paddingBottom="20dp"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    android:paddingTop="20dp"
    android:gravity="center"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.bandmusic.bandmusic.Paneles_Laterales.Asignar_Jurado">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignStart="@+id/textView8"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="38dp"
                    android:gravity="center"
                    app:srcCompat="@drawable/regitro_jurados" />

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignStart="@+id/imageView6"
                    android:layout_alignTop="@+id/imageView"
                    app:srcCompat="@drawable/jurado" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/imageView"
                    android:layout_marginTop="11dp"
                    android:text="Registra Jurados"
                    android:textColor="@color/black"
                    android:textSize="18dp" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView6"
                    android:layout_alignBottom="@+id/textView6"
                    android:layout_alignParentEnd="true"
                    android:text="Ingresa Evento"
                    android:textColor="@color/black"
                    android:textSize="18dp" />

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/imageView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignStart="@+id/imageView"
                    android:layout_below="@+id/textView6"
                    android:layout_marginTop="24dp"
                    app:srcCompat="@drawable/lista_calificar" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/imageView5"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="11dp"
                    android:text="Lista Jurados"
                    android:textColor="@color/black"
                    android:textSize="18dp" />

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/imageView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/textView8"
                    android:layout_alignStart="@+id/textView7"
                    android:layout_marginStart="15dp"
                    app:srcCompat="@drawable/criterios_evalua" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView8"
                    android:layout_alignBottom="@+id/textView8"
                    android:layout_alignStart="@+id/imageView6"
                    android:text="Lista Evento"
                    android:textColor="@color/black"
                    android:textSize="18dp" />

        </RelativeLayout>

</android.support.v4.widget.NestedScrollView>
